<link rel="stylesheet" href="/css/home.css">

<h2>
    Projects List
</h2>

<div class="projectsOuterDiv">
    
    <ul>
        <% for(project of projects){%>
            <li>
                <h4>
                    <%=project.name %>
                </h4>
                <p>
                    <%= project.author %>
                </p>
                <div class="iconDiv">
                    <button type="submit"><a href="/projects/details/<%=project._id %>"> View Details</a></button>
                    <div class="imgDiv">
                        <a href="/projects/delete/<%=project._id %>">
                            <img src="https://cdn-icons-png.flaticon.com/128/6861/6861362.png" alt="">
                        </a>
                       
                    </div>
                </div>
                
                
            </li>

        <%}%>
    </ul>
</div>

    